{
  "manifest_version": 2,
  "name": "C19-Search!",
  "version": "0.1.0",
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "run_at": "document_idle",
      "css": ["./contentStyles.css"],
      "js": ["./contentScript.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}